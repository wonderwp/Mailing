{
  "name": "wonderwp/mailing",
  "description": "WonderWp Mailing Component",
  "type": "library",
  "keywords": ["mailing", "mailer", "WonderWp", "WordPress"],
  "homepage": "https://github.com/wonderwp/Mailing",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeremy Desvaux",
      "homepage": "https://jdmweb.com"
    },
    {
      "name": "WonderWp Community",
      "homepage": "http://wonderwp.net/Contribution/Contributors.html"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "wonderwp/dependency-injection": "dev-feature/php_8",
    "wonderwp/http-foundation": "^dev-feature/increase_http_foundation_lib_version"
  },
  "autoload": {
    "psr-4": {
      "WonderWp\\Component\\Mailing\\": "src",
      "WonderWp\\Component\\Mailing\\Tests\\": "tests"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "minimum-stability": "dev"
}
